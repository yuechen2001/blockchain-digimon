      "translation": "",
      "description": "An evolved form of Ground Zero that has a new tracking function and upgraded power and range."
    },
    {
      "id": 142,
      "skill": "Ground Zero Kai",
      "translation": "Ground Zero Revision",
      "description": "Rotates its entire head to convert itself into a missile silo before launching a stronger organic missile."
    },
    {
      "id": 143,
      "skill": "Curse Breath",
      "translation": "",
      "description": "Exhales purple smoke from its mouth that paralyzes an opponent."
    },
    {
      "id": 144,
      "skill": "Skull Destroy",
      "translation": "",
      "description": "Fires a missile that causes a toxic explosion."
    },
    {
      "id": 145,
      "skill": "Death Nail",
      "translation": "",
      "description": "Slashes the enemy with the claws of one hand."
    }
  ],
